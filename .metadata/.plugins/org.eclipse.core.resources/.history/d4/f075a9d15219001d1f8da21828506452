package Lesson_2;

public class Java_operators {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		int mathGrade = 88;
		int scienceGrade = 90;
		int chemGrade = 95;
		double averageGrade = (mathGrade + scienceGrade + chemGrade) / 3;
		System.out.println("average grade is: " + averageGrade + ".");
		

	}

}
